<p style="color: green"><%= notice %></p>

<h1>Etiquetas en Food's Picture</h1>
<%= link_to "Nueva etiqueta", new_tag_path, class: "btn btn-primary mb-3" %>

<div id="tags">
  <% @tags.each do |tag| %>
    <%= render tag %>
    <p><%= link_to "Ver detalles", tag, class: "text-muted" %></p>
  <% end %>
</div>
